{
  "name": "cobol-check-dialect",
  "displayName": "cobol-check-dialect-for-cobol",
  "description": "Autocomplete, highlighting and diagnostics for COBOL CHECK",
  "author": "Broadcom",
  "license": "EPL-2.0",
  "version": "0.0.1",
  "preview": false,
  "publisher": "BroadcomMFD",
  "keywords" : [
    "cobol check",
    "cobol",
    "mainframe",
    "zowe"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "resources/logo.png",
  "activationEvents": [
    "onCommand:cobol-lsp.smart-tab",
    "onCommand:cobol-lsp.dialect.register",
    "onLanguage:cobol",
    "workspaceContains:/.c4z"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  },
  "extensionDependencies": ["BroadcomMFD.cobol-language-support"]
}
